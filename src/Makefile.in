TOPOBJDIR = ../..
SRCDIR    = @srcdir@
VPATH     = @srcdir@
MODULE    = libwine_port.a

C_SRCS = \
	getopt.c \
	getopt1.c \
	getpagesize.c \
	interlocked.c \
	lstat.c \
	memcpy_unaligned.c \
	memmove.c \
	mkstemps.c \
	pread.c \
	pwrite.c \
	spawn.c \
	statfs.c \
	strcasecmp.c \
	strerror.c \
	strncasecmp.c \
	usleep.c

all: $(MODULE)

@MAKE_RULES@

$(MODULE): $(OBJS) Makefile.in
	$(RM) $@
	$(AR) $@ $(OBJS)
	$(RANLIB) $@

